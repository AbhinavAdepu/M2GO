#import "Global.h"


@implementation Global
static SEL g_tSel;
static UIViewController *g_cntrl;
//set the Selector
+(void)setSelector:(SEL)t_sel
{
	g_tSel = t_sel;
	
}
+(SEL)getSelector
{
	return g_tSel;
}

//set the Handler
+(void)setHandler:(UIViewController *)handler
{
	g_cntrl = handler;
}
+(UIViewController *)getHandler
{
	return g_cntrl;
}
@end
